FROM python:3.10

ENV PYTHONNUNBUFFERED=1

WORKDIR /code/

COPY requirements.txt .

RUN pip3 install -r  requirements.txt

COPY . .

# RUN python manage.py collectstatic --noinput
RUN python manage.py migrate


EXPOSE 8000


CMD ["python3","manage.py", "runserver", "0.0.0.0:8000"]



